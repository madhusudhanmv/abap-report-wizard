   <exception CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="DEFINE_JOIN" SCONAME="JOIN_CNTRL_CALL_ERROR" VERSION="1" LANGU="E" MTDTYPE="0" EDITORDER="1 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000"/>
   <source>method define_join.

* Types

  types: &quot;tindx  type aqs_tindx,       &quot; Textindex
         ty_headsg_text(63) type c.   &quot; Functional Area Text.

  single_read = abap_false.
  if join_attributes-headsg is initial.
    join_attributes-headsg-art     = &apos;J&apos;.
    join_attributes-headsg-state   = &apos;U&apos;.
  endif.
  if join_attributes-mode_input is initial.
    join_attributes-mode_input     = 1.
  endif.

  call function &apos;RSAQ_DJ_DEFINE_JOIN_CNTRL&apos;
    exporting
*     ACT_WORKSPACE_INPUT       = &apos; &apos;
      sgname_input              = &apos; &apos;
      caller_id_input           = &apos;Q&apos;
      grafic_input              = &apos;X&apos;
    tables
      clogsg_input              = join_attributes-clogsg
      dbsa_input                = join_attributes-dbsa
      dbob_input                = join_attributes-dbob
      dbos_input                = join_attributes-dbos
      dbif_input                = join_attributes-dbif
      dbsf_input                = join_attributes-dbsf
      dbsg_input                = join_attributes-dbsg    &quot; &lt;- That is all I care about!
      dban_input                = join_attributes-dban    &quot; &lt;- That is all I care about!
      dbjt_input                = join_attributes-dbjt    &quot; &lt;- That is all I care about!
      dbjc_input                = join_attributes-dbjc    &quot; &lt;- That is all I care about!
      dbzt_input                = join_attributes-dbzt
      dbzc_input                = join_attributes-dbzc
      dbzl_input                = join_attributes-dbzl
      dbdp_input                = join_attributes-dbdp
      dbpa_input                = join_attributes-dbpa
      dbwr_input                = join_attributes-dbwr
      dbar_input                = join_attributes-dbar
      dbft_input                = join_attributes-dbft
      sgtext_input              = join_attributes-sgtext  &quot; &lt;- That is all I care about!
      exdbfi_input              = join_attributes-exdbfi
      ttab_input                = join_attributes-ttab    &quot; &lt;- That is all I care about!
    changing
      headsg_input              = join_attributes-headsg
      mode_input                = join_attributes-mode_input
      maxsg_tindx_input         = join_attributes-maxsg_tindx
   exceptions
     cancelled                 = 1
     others                    = 2.

  if sy-subrc &lt;&gt; 0.
    raise join_cntrl_call_error.
  else.
    join_defined = abap_true.
  endif.

  set_join_aliases( ).

endmethod.</source>
