  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="POPULATE_INITIALIZATION" VERSION="1" LANGU="E" DESCRIPT="Populate initialization" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="POPULATE_INITIALIZATION" SCONAME="ALIAS" VERSION="1" LANGU="E" DESCRIPT="SAP Query (S): Table name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070925" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="AQS_TNAME"/>
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="POPULATE_INITIALIZATION" SCONAME="TITLE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <source>method populate_initialization.

  data:
     wa_line                   type string,
     l_string                  type string,
     l_table                   type tabname,
     wa_dban                   type aqdban.

* Distinguish between real and fake alias
  read table join_attributes-dban into wa_dban with key alias = alias.
  if sy-subrc = 0.
    l_table = wa_dban-table.
  else.
    l_table = alias.
  endif.

  if initialization_code is initial.
    wa_line = &apos;Initialization.&apos;.
    append wa_line to initialization_code.
  endif.

  wa_line = &apos;select single max( ddtext ) from dd02t into&apos;.
  concatenate wa_line title &apos;where tabname    =&apos; into wa_line separated by space.
  concatenate &apos;&apos;&apos;&apos; l_table &apos;&apos;&apos;&apos; into l_string.
  concatenate wa_line l_string into wa_line separated by space.    append wa_line to initialization_code.
  wa_line = &apos;and ddlanguage = sy-langu. &quot;#EC *&apos;.
  shift wa_line right by 54 places in character mode.              append wa_line to initialization_code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="SET_JOIN_ALIASES" VERSION="1" LANGU="E" DESCRIPT="Set Aliases for all tables involved, even in single read" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070926" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method set_join_aliases.

* Variables
  data: l_count           type i,
        l_pattern         type char30,
        l_new_pattern     type char30.

* Workareas
  data: &quot;headsg            type aqhdsg,                   &quot; Head information for a subject
        wa_dban           type aqdban,
        wa_dbjt           type aqdbjt.    &quot; work area for dbjt,
* Field-symbols
  field-symbols: &lt;dbjc&gt;   type aqdbjc.

  join_attributes-dbjc_work = join_attributes-dbjc.

* If there is no Alias definition, create the default one (ABC)
  loop at join_attributes-dbjt into wa_dbjt.
    read table join_attributes-join_alias into wa_dban with key table = wa_dbjt-table.
    if sy-subrc ne 0.
      clear wa_dban.
      wa_dban-table = wa_dbjt-table.
      while wa_dban-alias  is initial.
        wa_dban-alias = sy-abcde+l_count(1).
        append wa_dban to join_attributes-join_alias.
        l_count = l_count + 1.
      endwhile.
    endif.
  endloop.

  loop at join_attributes-join_alias into wa_dban.
    concatenate wa_dban-table &apos;-&apos; into l_pattern.
    concatenate wa_dban-alias &apos;~&apos; into l_new_pattern.
    loop at join_attributes-dbjc_work assigning &lt;dbjc&gt;.
      replace: l_pattern in &lt;dbjc&gt;-lname with l_new_pattern in character mode,
               l_pattern in &lt;dbjc&gt;-rname with l_new_pattern in character mode.
    endloop.
  endloop.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="SET_SINGLE_READ" VERSION="1" LANGU="E" DESCRIPT="Set all variables to handle a single table read (or view)" EXPOSURE="2" STATE="1" EDITORDER="4 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="SET_SINGLE_READ" SCONAME="I_TABLE" VERSION="1" LANGU="E" DESCRIPT="Name of an ABAP Dictionary object" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TABNAME16"/>
   <source>method set_single_read.

  data: wa_dbjt      type aqdbjt.

  initialize_all( ).
  single_read = abap_true.
  wa_dbjt-table = i_table.
  append wa_dbjt to join_attributes-dbjt.

  set_join_aliases( ).

endmethod.</source>
  </method>
