*&amp;      Module  WZ_USER_COMMAND_WIZARD  INPUT
*&amp;---------------------------------------------------------------------*
MODULE wz_user_command_wizard INPUT.

  swf_okcode_get g_ok_code.
  g_save_ok_code = g_ok_code.
  CLEAR g_ok_code.

  CASE g_save_ok_code.
    WHEN &apos;JOIN&apos;.
      CLEAR o_code.

      CREATE OBJECT o_code.

      o_code-&gt;define_join( ).

      o_code-&gt;get_fields_involved( ).

      PERFORM free_containers.

    WHEN &apos;&gt;&gt;&apos;.   &quot; Validate possible actions
      CASE sy-dynnr.
        WHEN &apos;0200&apos;.
          IF it_source_type = &apos;Table&apos; AND zwiz_screen_fields-tablename IS INITIAL.

          ELSEIF it_source_type = &apos;Table-join&apos;.
            IF o_code IS NOT BOUND.
              MESSAGE e000(zwiz).
            ELSE.
              IF o_code-&gt;join_defined EQ abap_false.
                MESSAGE e000(zwiz).
              ENDIF.
            ENDIF.
          ENDIF.
        WHEN &apos;0300&apos;.
          IF o_selection-&gt;is_persistent( ) = abap_false.
            MESSAGE e001(zwiz).
          ENDIF.
        WHEN &apos;0400&apos;.
          IF o_output-&gt;is_persistent( ) = abap_false.
            MESSAGE e002(zwiz).
          ENDIF.
      ENDCASE.
  ENDCASE.

ENDMODULE.                 &quot; WZ_USER_COMMAND_WIZARD  INPUT</include_source>
