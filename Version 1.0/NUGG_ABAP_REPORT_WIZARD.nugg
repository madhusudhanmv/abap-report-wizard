  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="GET_IMPORTED_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Return the the selected fields for output and selection" EXPOSURE="2" STATE="1" EDITORDER="10 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="GET_IMPORTED_FIELDS" SCONAME="RE_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Table with Fields" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZWIZ_T_FIELDS"/>
   <source>METHOD get_imported_fields.

  DATA:
    lr_node TYPE REF TO cl_salv_node,
    lr_item TYPE REF TO cl_salv_item,
    lr_data TYPE REF TO data.

  DATA:
    lt_tree TYPE salv_t_nodes,
    ls_leaf TYPE salv_s_nodes.

  FIELD-SYMBOLS:
    &lt;lf_table_field&gt;    TYPE zwiz_table_field,
    &lt;lf_imported_field&gt; TYPE zwiz_fields.

  TRY.
      lt_tree = nodes-&gt;get_all_nodes( ).

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

  LOOP AT lt_tree INTO ls_leaf.
    lr_data = ls_leaf-node-&gt;get_data_row( ).

    ASSIGN lr_data-&gt;* TO &lt;lf_table_field&gt;.

    READ TABLE imported_fields ASSIGNING &lt;lf_imported_field&gt;
      WITH KEY alias     = &lt;lf_table_field&gt;-alias
               fieldname = &lt;lf_table_field&gt;-fieldname.

    CHECK sy-subrc = 0.

    TRY.
        lr_item = ls_leaf-node-&gt;get_item( &apos;SELECTION&apos; ).
        &lt;lf_imported_field&gt;-selection = lr_item-&gt;is_checked( ).

        lr_item = ls_leaf-node-&gt;get_item( &apos;PARAMETER&apos; ).
        &lt;lf_imported_field&gt;-parameter = lr_item-&gt;is_checked( ).

        lr_item = ls_leaf-node-&gt;get_item( &apos;OBLIGATORY&apos; ).
        &lt;lf_imported_field&gt;-obligatory = lr_item-&gt;is_checked( ).

      CATCH cx_salv_msg INTO salv_msg.
        MESSAGE salv_msg TYPE &apos;E&apos;.
    ENDTRY.
  ENDLOOP.

  re_fields[] = imported_fields[].

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="HANDLE_CHECKBOX_CHANGE" VERSION="1" LANGU="E" DESCRIPT="Handle Checkbox Change" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " AUTHOR="ALEXANDRE" CREATEDON="20070925" CHANGEDBY="ALEXANDRE" CHANGEDON="20070925" MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TREE" REFCMPNAME="CHECKBOX_CHANGE" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="HANDLE_CHECKBOX_CHANGE" SCONAME="COLUMNNAME" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " AUTHOR="ALEXANDRE" CREATEDON="20070925" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="HANDLE_CHECKBOX_CHANGE" SCONAME="NODE_KEY" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " AUTHOR="ALEXANDRE" CREATEDON="20070925" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="HANDLE_CHECKBOX_CHANGE" SCONAME="CHECKED" VERSION="1" LANGU="E" DESCRIPT="Checkbox ist gefllt" CMPTYPE="1" MTDTYPE="1" EDITORDER="3 " DISPID="0 " AUTHOR="ALEXANDRE" CREATEDON="20070925" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD HANDLE_CHECKBOX_CHANGE.

  DATA:
    lr_node TYPE REF TO cl_salv_node,
    lr_item TYPE REF TO cl_salv_item.

  CHECK columnname = &apos;SELECTION&apos;.

  TRY.
      lr_node = nodes-&gt;get_node( node_key ).

      IF checked = abap_true.
        lr_item = lr_node-&gt;get_item( &apos;PARAMETER&apos; ).
        lr_item-&gt;set_editable( abap_true ).

        lr_item = lr_node-&gt;get_item( &apos;OBLIGATORY&apos; ).
        lr_item-&gt;set_editable( abap_true ).
      ELSE.
        lr_item = lr_node-&gt;get_item( &apos;PARAMETER&apos; ).
        lr_item-&gt;set_editable( abap_false ).
        lr_item-&gt;set_checked( abap_false ).

        lr_item = lr_node-&gt;get_item( &apos;OBLIGATORY&apos; ).
        lr_item-&gt;set_editable( abap_false ).
        lr_item-&gt;set_checked( abap_false ).
      ENDIF.

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="IS_PERSISTENT" VERSION="1" LANGU="E" DESCRIPT="Check at least one field have been checked" EXPOSURE="2" STATE="1" EDITORDER="13 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="IS_PERSISTENT" SCONAME="RE_PERSISTENT" VERSION="1" LANGU="E" DESCRIPT="Checkbox (yes or no)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="TDBOOL"/>
   <source>METHOD IS_PERSISTENT.

  DATA:
    lr_node TYPE REF TO cl_salv_node,
    lr_item TYPE REF TO cl_salv_item.

  DATA:
    lt_tree TYPE salv_t_nodes,
    ls_leaf TYPE salv_s_nodes.

  TRY.
      lt_tree = nodes-&gt;get_all_nodes( ).

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

  LOOP AT lt_tree INTO ls_leaf.
    TRY.
        lr_item = ls_leaf-node-&gt;get_item( &apos;SELECTION&apos; ).
        IF lr_item-&gt;is_checked( ) = abap_true.
          re_persistent = abap_true. RETURN.
        ENDIF.

      CATCH cx_salv_msg INTO salv_msg.
        MESSAGE salv_msg TYPE &apos;E&apos;.
    ENDTRY.
  ENDLOOP.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="PROCESS_COLUMNS" VERSION="1" LANGU="E" DESCRIPT="Process Treeview Columns" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDBY="RICH" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS_COLUMNS.

  DATA lr_column TYPE REF TO cl_salv_column.

  columns = tree-&gt;get_columns( ).

  TRY.
      lr_column = columns-&gt;get_column( &apos;FIELDNAME&apos; ).
      lr_column-&gt;set_output_length( 12 ).

      lr_column = columns-&gt;get_column( &apos;SELECTION&apos; ).
      lr_column-&gt;set_output_length( 5 ).

      lr_column = columns-&gt;get_column( &apos;PARAMETER&apos; ).
      lr_column-&gt;set_output_length( 5 ).

      lr_column = columns-&gt;get_column( &apos;OBLIGATORY&apos; ).
      lr_column-&gt;set_output_length( 5 ).

      lr_column = columns-&gt;get_column( &apos;ALIAS&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;TABNAME&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;TABLE_TEXT&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;FIELD_TEXT&apos; ).
