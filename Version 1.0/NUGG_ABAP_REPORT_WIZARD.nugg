  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE" VERSION="1" LANGU="E" DESCRIPT="Get code for program (ALL)" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code.

  data: it_code_tmp      type rswsourcet.

* Header
  it_code_tmp = get_code_header( ).
  append lines of it_code_tmp to code.

* Tables Statement
  it_code_tmp = get_code_tables( ).
  append lines of it_code_tmp to code.

* Selection screen
  it_code_tmp = get_code_selection_screen( ).
  append lines of it_code_tmp to code.

* Data declaration
  it_code_tmp = get_code_data( ).
  append lines of it_code_tmp to code.

* Event handler class
  it_code_tmp = get_code_event_handler( ).
  append lines of it_code_tmp to code.

* Initialization (if exists)
  it_code_tmp = initialization_code.
  append lines of it_code_tmp to code.
  append space to code.

* Select statement
  it_code_tmp = get_code_select( ).
  append lines of it_code_tmp to code.

* End of Selection event
  it_code_tmp = get_code_end_of_selection( ).
  append lines of it_code_tmp to code.

  call function &apos;PRETTY_PRINTER&apos;
    exporting
      inctoo = space
    tables
      ntext  = code
      otext  = code
    exceptions
      others = 0.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_DATA" VERSION="1" LANGU="E" DESCRIPT="Get code for Data Declaration" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_DATA" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code_data.

  data: wa_line                   type string,
        l_string1                 type string,
        l_string2                 type string,
        l_length                  type i,
        l_length_max              type i,
        l_offset                  type i,
        wa_field                  type zwiz_fields.

  constants: c_dash               type char1 value &apos;-&apos;.

  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  wa_line = &apos;*       D A T A   D E C L A R A T I O N       *&apos;.   append wa_line to code.
  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  clear wa_line. append wa_line to code.

  if options-createevthdlr eq abap_true.
    wa_line = &apos;CLASS lcl_event_handler definition deferred.&apos;.
    append wa_line to code.
  endif.
  clear wa_line. append wa_line to code.

  wa_line = &apos;* Types&apos;.
  append wa_line to code.
  wa_line = &apos;TYPES: begin of TY_TARGET,&apos;.
  append wa_line to code.
* Get max size of field
  loop at fields into wa_field where output = abap_true.
    l_length = strlen( wa_field-target_fieldname ).
    if l_length &gt; l_length_max.
      l_length_max = l_length.
    endif.
  endloop.
  l_length_max = l_length_max + 2.
  loop at fields into wa_field where output = abap_true.
    concatenate wa_field-tabname wa_field-fieldname into l_string1 separated by c_dash.
    concatenate &apos;TYPE&apos; l_string1 into l_string2 separated by space.
    concatenate l_string2 &apos;,&apos; into l_string2.
    l_offset = l_length_max - strlen( wa_field-target_fieldname ).
    shift l_string2 by l_offset places right in character mode.
    concatenate wa_field-target_fieldname l_string2 into wa_line. &quot;respecting blanks.
    shift wa_line by 6 places right in character mode.
    append wa_line to code.
  endloop.

  wa_line = &apos;end of TY_TARGET.&apos;.
  shift wa_line by 6 places right in character mode.
  append wa_line to code.
  clear wa_line. append wa_line to code.

  wa_line = &apos;* Internal tables and work areas&apos;.
  append wa_line to code.
  wa_line = &apos;DATA:&apos;.
  append wa_line to code.
  wa_line = &apos;IT_DATA        type table of TY_TARGET,&apos;.
  shift wa_line by 6 places right in character mode.
  append wa_line to code.
  wa_line = &apos;WA_DATA        like line of IT_DATA,&apos;.
  shift wa_line by 6 places right in character mode.
  append wa_line to code.
  wa_line = &apos;GV_TITLE       type LVC_TITLE,&apos;.
  shift wa_line by 6 places right in character mode.
  append wa_line to code.
  wa_line = &apos;r_alv_template type ref to ZCL_BT_ALV_TEMPLATE.&apos;.
  shift wa_line by 6 places right in character mode.
  append wa_line to code.

  clear wa_line. append wa_line to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_END_OF_SELECTION" VERSION="1" LANGU="E" DESCRIPT="Get code for event End-of-Selection" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_END_OF_SELECTION" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code_end_of_selection.

  data: wa_line                   type string,
        l_string                  type string.

  concatenate &apos;&apos;&apos;&apos; report_title &apos;&apos;&apos;&apos; into l_string.
  concatenate l_string &apos;(TIT).&apos;      into l_string.

  wa_line = &apos;*---------------------------------------------*&apos;.          append wa_line to code.
  wa_line = &apos;*        A L V   P R O C E S S I N G          *&apos;.          append wa_line to code.
  wa_line = &apos;*---------------------------------------------*&apos;.          append wa_line to code.
  wa_line = &apos;END-OF-SELECTION.&apos;.                                        append wa_line to code.
  append space to code.
  concatenate &apos;GV_TITLE&apos; &apos;=&apos; l_string into wa_line separated by space.
  append wa_line to code.
  append space to code.

  wa_line = &apos;*Build ALV Template Object&apos;.                 append wa_line to code.
  wa_line = &apos;zcl_bt_alv_template=&gt;factory(&apos;.              append wa_line to code.
  wa_line = &apos;exporting  im_syrepid = sy-repid&apos;.
  shift wa_line by 15 places right in character mode.     append wa_line to code.
  wa_line = &apos;im_lheader = gv_title&apos;.
  shift wa_line by 26 places right in character mode.     append wa_line to code.
  if options-allowlayoutsave eq abap_true.
    wa_line = &apos;im_allowls = abap_true  &quot; Allow layout save&apos;.
    shift wa_line by 26 places right in character mode.   append wa_line to code.
  else.
    wa_line = &apos;im_allowls = abap_false  &quot; Doesn&apos;&apos;&apos;&apos;t allow layout save&apos;.
    shift wa_line by 26 places right in character mode.   append wa_line to code.
  endif.
  wa_line = `importing  ex_alvo    = r_alv_template`.
  shift wa_line by 15 places right in character mode.     append wa_line to code.
  wa_line = `changing   ch_datatab = it_data ).`.
  shift wa_line by 15 places right in character mode.     append wa_line to code.

  append space to code.

  if options-createevthdlr eq abap_true.
    wa_line = &apos;* Handle events&apos;.                          append wa_line to code.
    wa_line = &apos;set handler lcl_event_handler=&gt;on_double_click for r_alv_template-&gt;r_events.&apos;.  append wa_line to code.
  endif.
  wa_line = &apos;* ALV Table Display&apos;.                                      append wa_line to code.
  wa_line = &apos;r_alv_template-&gt;r_table-&gt;display( ).&apos;.                              append wa_line to code.

  append space to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_EVENT_HANDLER" VERSION="1" LANGU="E" DESCRIPT="Get code for Local Event Handler" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_EVENT_HANDLER" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>METHOD get_code_event_handler.

  DATA: wa_line                   TYPE string.

  IF options-createevthdlr = abap_true.
    wa_line = &apos;*---------------------------------------------*&apos;.   APPEND wa_line TO code.
    wa_line = &apos;*   CLASS lcl_event_handler DEFINITION        *&apos;.   APPEND wa_line TO code.
    wa_line = &apos;*---------------------------------------------*&apos;.   APPEND wa_line TO code.
    wa_line = &apos;class lcl_event_handler definition.&apos;.                                         APPEND wa_line TO code.
    wa_line = &apos;public section.&apos;. SHIFT wa_line BY 2 PLACES RIGHT IN CHARACTER MODE.          APPEND wa_line TO code.
    wa_line = &apos;class-methods:&apos;.  SHIFT wa_line BY 4 PLACES RIGHT IN CHARACTER MODE.          APPEND wa_line TO code.
    wa_line = &apos;on_double_click for event double_click of cl_salv_events_table&apos;.
    SHIFT wa_line BY 8 PLACES RIGHT IN CHARACTER MODE.          APPEND wa_line TO code.
    wa_line = &apos;importing row column.&apos;.
    SHIFT wa_line BY 20 PLACES RIGHT IN CHARACTER MODE.          APPEND wa_line TO code.
    wa_line = &apos;endclass.                    &quot;lcl_event_handler DEFINITION&apos;.                  APPEND wa_line TO code.
    wa_line = &apos;*---------------------------------------------*&apos;.   APPEND wa_line TO code.
    wa_line = &apos;*   CLASS lcl_event_handler IMPLEMENTATION    *&apos;.                              APPEND wa_line TO code.
    wa_line = &apos;*---------------------------------------------*&apos;.   APPEND wa_line TO code.
    wa_line = &apos;class lcl_event_handler implementation.&apos;.                                     APPEND wa_line TO code.
    wa_line = &apos;method on_double_click.&apos;. SHIFT wa_line BY 2 PLACES RIGHT IN CHARACTER MODE.  APPEND wa_line TO code.
    wa_line = &apos;* Insert double click functionality here&apos;.                                    APPEND wa_line TO code.
    wa_line = &apos;endmethod.                    &quot;ondoubleclick&apos;.
    SHIFT wa_line BY 2 PLACES RIGHT IN CHARACTER MODE.  APPEND wa_line TO code.
    wa_line = &apos;endclass.                    &quot;lcl_event_handler IMPLEMENTATION&apos;.              APPEND wa_line TO code.
    CLEAR wa_line. APPEND wa_line TO code.
  ENDIF.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_HEADER" VERSION="1" LANGU="E" DESCRIPT="Get code for Header of the report" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_HEADER" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method GET_CODE_HEADER.

  data: wa_line                   type string,
        wa_end_line               type char100,
        wa_date                   type char10,
        wa_time                   type char10.

  wa_line = &apos;*----------------------------------------------------------------------*&apos;. append wa_line to code.
  concatenate &apos;* Report generated by ABAP Report Wizard v.&apos; c_version into wa_end_line separated by space.
  wa_end_line+71(1) = &apos;*&apos;.
  wa_line = wa_end_line.
  append wa_line to code.

  wa_line = &apos;*----------------------------------------------------------------------*&apos;. append wa_line to code.

  concatenate &apos;* Report Name:&apos; report_title into wa_line separated by space.
  wa_end_line = wa_line.
  wa_end_line+71(1) = &apos;*&apos;.
  wa_line = wa_end_line.
  append wa_line to code.

  concatenate &apos;* Created by:&apos; sy-uname into wa_line separated by space.
  wa_end_line = wa_line.
  wa_end_line+71(1) = &apos;*&apos;.
  wa_line = wa_end_line.
  append wa_line to code.

  write sy-datlo to wa_date.
  write sy-timlo to wa_time.
  concatenate &apos;* Created on:&apos;
              wa_date
              &apos;at&apos;
              wa_time
              sy-zonlo into wa_line separated by space.
  wa_end_line = wa_line.
  wa_end_line+71(1) = &apos;*&apos;.
  wa_line = wa_end_line.
  append wa_line to code.

  wa_line = &apos;*----------------------------------------------------------------------*&apos;. append wa_line to code.
  append space to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_INSTRUCTIONS" VERSION="1" LANGU="E" DESCRIPT="Get user instructions ( Not used since the Wizard does it )" EXPOSURE="2" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070924" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_INSTRUCTIONS" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method GET_CODE_INSTRUCTIONS.

  data: wa_line                   type string.

  wa_line = &apos;*-----------------------------------------------------------------*&apos;. append wa_line to code.
  wa_line = &apos;*            * * *  I N S T R U C T I O N S  * * *                *&apos;. append wa_line to code.
  wa_line = &apos;*-----------------------------------------------------------------*&apos;. append wa_line to code.
  wa_line = &apos;* The ABAP wizard generated predefined report structure           *&apos;. append wa_line to code.
  wa_line = &apos;* Next Steps:                                                     *&apos;. append wa_line to code.
  wa_line = &apos;* 1 - Point the seletion parameters to their dictionary reference;*&apos;. append wa_line to code.
  wa_line = &apos;* 2 - Double click on text elements to generate them;             *&apos;. append wa_line to code.
  wa_line = &apos;* 3 - Program your own data manipulation, if required;(optional)  *&apos;. append wa_line to code.
  wa_line = &apos;* 4 - Program your own event handling, if you chose so;(optional) *&apos;. append wa_line to code.
