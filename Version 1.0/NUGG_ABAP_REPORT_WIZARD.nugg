  <attribute CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="R_TABLE" VERSION="1" LANGU="E" DESCRIPT="Basis Class for Simple Tables" EXPOSURE="2" STATE="1" EDITORDER="10 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" ATTDECLTYP="0" ATTEXPVIRT="0" TYPTYPE="3" TYPE="CL_SALV_TABLE" SRCROW1="0 " SRCCOLUMN1="0 " SRCROW2="0 " SRCCOLUMN2="0 " R3RELEASE="700" TYPESRC_LENG="0 "/>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="Constructor" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="2" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="CONSTRUCTOR" SCONAME="IM_SYREPID" VERSION="1" LANGU="E" DESCRIPT="ABAP Program: Current Main Program" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SYREPID"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="CONSTRUCTOR" SCONAME="IM_LHEADER" VERSION="1" LANGU="E" DESCRIPT="ALV Control: Title bar text" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="LVC_TITLE"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="CONSTRUCTOR" SCONAME="IM_ALLOWLS" VERSION="1" LANGU="E" DESCRIPT="Allow Layout Save" CMPTYPE="1" MTDTYPE="2" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ABAP_BOOL"/>
   <source>method CONSTRUCTOR.

 me-&gt;program_id        = im_syrepid.
 me-&gt;list_header       = im_lheader.
 me-&gt;allow_layout_save = im_allowls.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" VERSION="1" LANGU="E" DESCRIPT="Build ALV  Template Object" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="1" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" SCONAME="IM_SYREPID" VERSION="1" LANGU="E" DESCRIPT="ABAP Program: Current Main Program" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SYREPID"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" SCONAME="IM_LHEADER" VERSION="1" LANGU="E" DESCRIPT="ALV Control: Title bar text" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="LVC_TITLE"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" SCONAME="IM_ALLOWLS" VERSION="1" LANGU="E" DESCRIPT="Allow Layout Save" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ABAP_BOOL"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" SCONAME="EX_ALVO" VERSION="1" LANGU="E" DESCRIPT="ALV Template Class" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070904" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="3" TYPE="ZCL_BT_ALV_TEMPLATE"/>
   <parameter CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="FACTORY" SCONAME="CH_DATATAB" VERSION="1" LANGU="E" DESCRIPT="Data Tab" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="TABLE"/>
   <source>method FACTORY.

* Create the object and pass initial values.
  CREATE OBJECT ex_alvo
             EXPORTING im_syrepid = im_syrepid
                       im_lheader = im_lheader
                       im_allowls = im_allowls.

* Create the main ALV object
  TRY.
      cl_salv_table=&gt;factory(
        IMPORTING
           r_salv_table   = ex_alvo-&gt;r_table
        CHANGING
           t_table        = ch_datatab
             ).
    CATCH cx_salv_msg .
  ENDTRY.

* Pass reference to the attribute DATA_TAB
  GET REFERENCE OF ch_datatab INTO ex_alvo-&gt;data_tab.

* Get/Set functions object
  ex_alvo-&gt;set_alv_functions( ).

*  Get/Set display functions object
  ex_alvo-&gt;set_alv_disp_settings( ).

*  Get/Set columns object
  ex_alvo-&gt;set_alv_column( ).

* Get/Set layout object
  ex_alvo-&gt;set_alv_layout( ).

* Get/Set Header
  ex_alvo-&gt;set_alv_header( ).

* Get/Set footer
  ex_alvo-&gt;set_alv_footer( ).

* Get events object.
  ex_alvo-&gt;r_events = ex_alvo-&gt;r_table-&gt;get_event( ).

* Get Sorts object
  ex_alvo-&gt;r_sorts = ex_alvo-&gt;r_table-&gt;get_sorts( ).

* Get filters object
  ex_alvo-&gt;r_filters = ex_alvo-&gt;r_table-&gt;get_filters( ).

* Get selections object
  ex_alvo-&gt;r_selections = ex_alvo-&gt;r_table-&gt;get_selections( ).

* Get Aggregations object
  ex_alvo-&gt;r_aggregations = ex_alvo-&gt;r_table-&gt;get_aggregations( ).

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_COLUMN" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Column Settings" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method SET_ALV_COLUMN.

  DATA: lr_column TYPE REF TO cl_salv_column_table.

* Get columns object
  me-&gt;r_columns = me-&gt;r_table-&gt;get_columns( ).
  me-&gt;r_columns-&gt;set_optimize( abap_true ).

* If there is a &quot;Client&quot; column, set it as techical.
  TRY.
      lr_column ?= me-&gt;r_columns-&gt;get_column( &apos;MANDT&apos; ).
      lr_column-&gt;set_technical( abap_true ).
    CATCH cx_salv_not_found.
  ENDTRY.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_DISP_SETTINGS" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Display Settings" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method SET_ALV_DISP_SETTINGS.

* Set display settings
  me-&gt;r_disp_settings = me-&gt;r_table-&gt;get_display_settings( ).
  me-&gt;r_disp_settings-&gt;set_striped_pattern( abap_true ).
  me-&gt;r_disp_settings-&gt;set_list_header( list_header ).

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_FOOTER" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Footer Settings" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD set_alv_footer.

  DATA: lr_grid  TYPE REF TO cl_salv_form_layout_grid,
        lr_label TYPE REF TO cl_salv_form_label,
        lr_text  TYPE REF TO cl_salv_form_text.

  CREATE OBJECT lr_grid.

* Show User name
  lr_label = lr_grid-&gt;create_label( row = 1 column = 1
                                    text = &apos;User Id:&apos; tooltip = &apos;User Id&apos; ).
  lr_text = lr_grid-&gt;create_text( row = 1 column = 2
                                  text = sy-uname  tooltip = sy-uname ).
* Show Sys Id
  lr_label = lr_grid-&gt;create_label( row = 2 column = 1
                                    text = &apos;System Id:&apos; tooltip = &apos;System Id&apos; ).
  lr_text = lr_grid-&gt;create_text( row = 2 column = 2
                                  text = sy-sysid tooltip = sy-sysid ).

* Show Sys Id
  lr_label = lr_grid-&gt;create_label( row = 3 column = 1
                                    text = &apos;Client:&apos; tooltip = &apos;Client&apos; ).
  lr_text = lr_grid-&gt;create_text( row = 3 column = 2
                                  text = sy-mandt tooltip = sy-mandt ).

* Get the number of lines from the internal table.
  FIELD-SYMBOLS: &lt;fs_datatab&gt; TYPE table.
  DATA: lines TYPE i.
  ASSIGN me-&gt;data_tab-&gt;* TO &lt;fs_datatab&gt;.
  lines = LINES( &lt;fs_datatab&gt; ).

  lr_label = lr_grid-&gt;create_label( row = 4 column = 1
                                    text = &apos;# of Rows:&apos; tooltip = &apos;# of Rows&apos; ).
  lr_text = lr_grid-&gt;create_text( row = 4 column = 2
                                  text = lines tooltip = lines ).

* Set the element end_of_list
  me-&gt;r_table-&gt;set_end_of_list( lr_grid ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_FUNCTIONS" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Functions" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method SET_ALV_FUNCTIONS.

* Set functions
  me-&gt;r_functions =  me-&gt;r_table-&gt;get_functions( ).
  me-&gt;r_functions-&gt;set_all( abap_true ).


endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_HEADER" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Header Settings" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD set_alv_header.

* Set the top of list
  DATA: lr_grid  TYPE REF TO cl_salv_form_layout_grid,
        lr_gridx TYPE REF TO cl_salv_form_layout_grid,
        lr_flow  TYPE REF TO cl_salv_form_layout_flow,
        lr_pict  TYPE REF TO cl_salv_form_picture,
        lr_logo  TYPE REF TO cl_salv_form_layout_logo,
        lr_label TYPE REF TO cl_salv_form_label,
        lr_text  TYPE REF TO cl_salv_form_text.

  CREATE OBJECT lr_grid.

  lr_grid-&gt;create_header_information( row = 1  column = 1
                                      text = me-&gt;list_header
                                      tooltip = me-&gt;list_header ).

  lr_gridx = lr_grid-&gt;create_grid( row = 2  column = 1  ).
  lr_label = lr_gridx-&gt;create_label( row = 2 column = 1
                                    text = &apos;Program:&apos; tooltip = &apos;Program&apos; ).
  lr_text  = lr_gridx-&gt;create_text(  row = 2 column = 2
                                     text = program_id  tooltip = program_id ).
  lr_label = lr_gridx-&gt;create_label( row = 3 column = 1
                                     text = &apos;Run Date:&apos; tooltip = &apos;Run Date&apos; ).
  lr_text  = lr_gridx-&gt;create_text(  row = 3 column = 2
                                     text = sy-datum  tooltip = sy-datum ).
  lr_label = lr_gridx-&gt;create_label( row = 4 column = 1
                                     text = &apos;Run Time:&apos; tooltip = &apos;Run Time&apos; ).
  lr_text  = lr_gridx-&gt;create_text(  row = 4 column = 2
                                     text = sy-uzeit tooltip = sy-uzeit ).

* Create logo layout, set grid content on left and logo image on right
  CREATE OBJECT lr_logo.
  lr_logo-&gt;set_left_content( lr_grid ).
  lr_logo-&gt;set_right_logo( me-&gt;picture_id ).

* Set the element top_of_list
  me-&gt;r_table-&gt;set_top_of_list( lr_logo ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ALV_TEMPLATE" CMPNAME="SET_ALV_LAYOUT" VERSION="1" LANGU="E" DESCRIPT="Get/Set Default ALV Layout Settings" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method SET_ALV_LAYOUT.

  IF me-&gt;allow_layout_save = abap_false.
    RETURN.
  ENDIF.

* Set layout specifications
  DATA: ls_key TYPE salv_s_layout_key.
  ls_key-report = me-&gt;program_id.

  me-&gt;r_layout = me-&gt;r_table-&gt;get_layout( ).
  me-&gt;r_layout-&gt;set_key( ls_key ).
  me-&gt;r_layout-&gt;set_default( abap_true ).
  me-&gt;r_layout-&gt;set_save_restriction( if_salv_c_layout=&gt;restrict_none ).

endmethod.</source>
  </method>
 </CLAS>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_001" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_001" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="UT" TDLINE="Welcome to the ABAP Report Wizard!"/>
  <TextLines TDFORMAT="AS"/>
  <TextLines TDFORMAT="AS" TDLINE="This Wizard will help you create a basic ABAP report following"/>
  <TextLines TDLINE="predefined programming standards."/>
  <TextLines TDFORMAT="AS" TDLINE="After the execution of this wizard, predefined ABAP code will be"/>
  <TextLines TDLINE="automatically generated what not only accelerates the report creation"/>
  <TextLines TDLINE="time but also guarantees that the company&apos;s standards are respected."/>
  <TextLines TDFORMAT="AS"/>
 </DOCV>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_002" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_002" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="HU" TDLINE="Define Report Type"/>
  <TextLines TDFORMAT="*"/>
  <TextLines TDFORMAT="*" TDLINE="During this step you define the report name and its data source. If"/>
  <TextLines TDLINE="table join is selected, the user will need to define the table joins"/>
  <TextLines TDLINE="using the SAP graphical join tool (the same used by ABAP Query and Query"/>
  <TextLines TDLINE="viewer)"/>
 </DOCV>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_003" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_003" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="HU" TDLINE="Define Selection Criteria"/>
  <TextLines TDFORMAT="*"/>
  <TextLines TDFORMAT="*" TDLINE="During this step, you define the selection. Based on the table(s) used,"/>
  <TextLines TDLINE="a list of possible fields is proposed. Select the relevant fields to be"/>
  <TextLines TDLINE="used as Selection criteria and field selection."/>
  <TextLines TDFORMAT="*" TDLINE="The selection screen and the SQL Select statement are going to be"/>
  <TextLines TDLINE="affected by this selection."/>
 </DOCV>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_004" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_004" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="HU" TDLINE="Define Data to be Selected"/>
  <TextLines TDFORMAT="*"/>
  <TextLines TDFORMAT="*" TDLINE="During this step, you can define the following fo the selection"/>
  <TextLines TDLINE="criteria:"/>
  <TextLines TDFORMAT="B2" TDLINE="set as parameters (default or NO = select-option);"/>
  <TextLines TDFORMAT="B2" TDLINE="set mandatory."/>
  <TextLines TDFORMAT="*"/>
 </DOCV>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_005" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_005" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="HU" TDLINE="Report Options"/>
  <TextLines TDFORMAT="*" TDLINE="Here you can define additional options for the report, like:"/>
  <TextLines TDFORMAT="B2" TDLINE="all code in one include or split in multiple includes;"/>
  <TextLines TDFORMAT="B2" TDLINE="create session for event handler (shell);"/>
  <TextLines TDFORMAT="B2" TDLINE="Allow users to save the report layout"/>
 </DOCV>
 <DOCV OBJNAME="DTZWIZ_BT_REPORT_WIZARD_006" ID="DT" OBJECT="ZWIZ_BT_REPORT_WIZARD_006" TYP="E" DOKVERSION="0001">
  <TextLines TDFORMAT="HU" TDLINE="It is done!"/>
  <TextLines TDFORMAT="AS" TDLINE="You now completed the wizard. Once you click on FINISH, the following"/>
  <TextLines TDLINE="will be created (from the point where you called this wizard):"/>
  <TextLines TDFORMAT="N1" TDLINE="Selection screen definition (in same code or separate include);"/>
  <TextLines TDFORMAT="N1" TDLINE="Data definition for internal table and work area (in same code or"/>
  <TextLines TDLINE="separate include);"/>
  <TextLines TDFORMAT="N1" TDLINE="Start-of-selection event with the requested SQL Select"/>
  <TextLines TDFORMAT="N1" TDLINE="End-of-selection even with the ALV call as per company&apos;s standards."/>
  <TextLines TDFORMAT="AS"/>
  <TextLines TDFORMAT="AS" TDLINE="You are now free to change the code at will. Please follow defined"/>
  <TextLines TDLINE="programming standards."/>
  <TextLines TDFORMAT="AS"/>
  <TextLines TDFORMAT="AS" TDLINE="Good luck!"/>
 </DOCV>
