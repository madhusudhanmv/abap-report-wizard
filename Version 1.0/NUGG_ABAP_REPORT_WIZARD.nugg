  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="DISPLAY" VERSION="1" LANGU="E" DESCRIPT="Display Treeview" EXPOSURE="2" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD DISPLAY.

  IF container IS NOT BOUND.
    IF cl_salv_tree=&gt;is_offline( ) = abap_false.
      CREATE OBJECT container
        EXPORTING
          container_name = &apos;CC_TREE&apos;.
    ENDIF.

    TRY.
        CALL METHOD cl_salv_tree=&gt;factory
          EXPORTING
            r_container = container
          IMPORTING
            r_salv_tree = tree
          CHANGING
            t_table     = gr_table_fields.
      CATCH cx_salv_error.
        EXIT.
    ENDTRY.

    process_nodes( ).

    process_tree_settings( ).

    process_functions( ).

    process_columns( ).

    process_events( ).

    tree-&gt;display( ).
  ENDIF.

