  <method CLSNAME="ZCL_BT_ABAP_SELECTION_FIELDS" CMPNAME="REFORMAT_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Reformat IT for Treeview Display" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD reformat_fields.

  DATA:
    ls_table_field TYPE zwiz_table_field,
    l_table_text   TYPE dd02t-ddtext.

  FIELD-SYMBOLS
    &lt;lf_imported_field&gt; TYPE zwiz_fields.

  LOOP AT imported_fields ASSIGNING &lt;lf_imported_field&gt;.
    AT NEW alias.
      SELECT SINGLE ddtext FROM dd02t INTO l_table_text
        WHERE tabname    = &lt;lf_imported_field&gt;-tabname
          AND ddlanguage = sy-langu.

      IF &lt;lf_imported_field&gt;-alias &lt;&gt; &lt;lf_imported_field&gt;-tabname.
        CONCATENATE &lt;lf_imported_field&gt;-alias l_table_text
          INTO l_table_text SEPARATED BY space.
      ENDIF.
    ENDAT.

    MOVE-CORRESPONDING &lt;lf_imported_field&gt; TO ls_table_field.

    ls_table_field-table_text = l_table_text.
    ls_table_field-field_text = &lt;lf_imported_field&gt;-scrtext_m.

    APPEND ls_table_field TO table_fields.

    AT END OF tabname.
      CLEAR l_table_text.
    ENDAT.
  ENDLOOP.

ENDMETHOD.</source>
  </method>
