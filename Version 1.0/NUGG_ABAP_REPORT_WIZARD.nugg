  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_FIELD_NODE" VERSION="1" LANGU="E" DESCRIPT="Add Field Node to Treeview" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_FIELD_NODE" SCONAME="IM_DATA_ROW" VERSION="1" LANGU="E" DESCRIPT="Table fields selection for treeview" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZWIZ_TABLE_FIELD"/>
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_FIELD_NODE" SCONAME="IM_RELATED_NODE" VERSION="1" LANGU="E" DESCRIPT="Node Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SALV_DE_NODE_KEY"/>
   <source>METHOD ADD_FIELD_NODE.

  DATA:
    l_value     TYPE lvc_value,
    l_key_image TYPE salv_de_tree_image.

  DATA:
    lr_node  TYPE REF TO cl_salv_node,
    lr_item  TYPE REF TO cl_salv_item.

  TRY.
      l_value = im_data_row-field_text.

      IF im_data_row-keyflag = abap_true.
        l_key_image = &apos;@3V@&apos;.
      ENDIF.

      CALL METHOD nodes-&gt;add_node
        EXPORTING
          related_node   = im_related_node
          relationship   = cl_gui_column_tree=&gt;relat_last_child
          data_row       = im_data_row
          collapsed_icon = l_key_image
          expanded_icon  = l_key_image
          text           = l_value
        RECEIVING
          node           = lr_node.

      lr_item = lr_node-&gt;get_item( &apos;OUTPUT&apos; ).
      lr_item-&gt;set_type( if_salv_c_item_type=&gt;checkbox ).
      lr_item-&gt;set_editable( abap_true ).

      IF im_data_row-keyflag = abap_true.
        lr_item = lr_node-&gt;get_hierarchy_item( ).
        lr_item-&gt;set_style( if_salv_c_tree_style=&gt;intensified ).
      ENDIF.

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_TABLE_NODE" VERSION="1" LANGU="E" DESCRIPT="Add Table Node to Treeview" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_TABLE_NODE" SCONAME="IM_DATA_ROW" VERSION="1" LANGU="E" DESCRIPT="Table fields selection for treeview" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZWIZ_TABLE_FIELD"/>
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="ADD_TABLE_NODE" SCONAME="RE_NODE_KEY" VERSION="1" LANGU="E" DESCRIPT="Node Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="SALV_DE_NODE_KEY"/>
   <source>METHOD add_table_node.

  DATA:
    lw_data_row    TYPE zwiz_table_field,
    lw_value       TYPE lvc_value,
    lw_table_image TYPE salv_de_tree_image.

  DATA:
    lr_node  TYPE REF TO cl_salv_node.

  TRY.
      lw_data_row = im_data_row.

      lw_value = lw_data_row-table_text.

      CLEAR: lw_data_row-tabname,
             lw_data_row-fieldname.

      lw_table_image = &apos;@3W@&apos;.

      CALL METHOD nodes-&gt;add_node
        EXPORTING
          related_node   = &apos;&apos;
          relationship   = cl_gui_column_tree=&gt;relat_last_child
          data_row       = lw_data_row
          collapsed_icon = lw_table_image
          expanded_icon  = lw_table_image
          row_style      = if_salv_c_tree_style=&gt;emphasized_positive
          text           = lw_value
        RECEIVING
          node           = lr_node.

      re_node_key = lr_node-&gt;get_key( ).

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="10 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="2" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="CONSTRUCTOR" SCONAME="IM_TABLE_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Table with Fields" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZWIZ_T_FIELDS"/>
   <source>METHOD CONSTRUCTOR.

  imported_fields[] = im_table_fields[].

  reformat_fields( ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="DISPLAY" VERSION="1" LANGU="E" DESCRIPT="Display Treeview" EXPOSURE="2" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD DISPLAY.

  IF container IS NOT BOUND.
    IF cl_salv_tree=&gt;is_offline( ) = abap_false.
      CREATE OBJECT container
        EXPORTING
          container_name = &apos;CC_SELECTION&apos;.
    ENDIF.

    TRY.
        CALL METHOD cl_salv_tree=&gt;factory
          EXPORTING
            r_container = container
          IMPORTING
            r_salv_tree = tree
          CHANGING
            t_table     = gr_table_fields.
      CATCH cx_salv_error.
        EXIT.
    ENDTRY.

    process_nodes( ).

    process_tree_settings( ).

    process_functions( ).

    process_columns( ).

    tree-&gt;display( ).
  ENDIF.

ENDMETHOD.</source>
  </method>
