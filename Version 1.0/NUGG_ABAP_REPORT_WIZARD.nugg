   <source>METHOD free.

  container-&gt;free( ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="GET_IMPORTED_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Return the the selected fields for output and selection" EXPOSURE="2" STATE="1" EDITORDER="9 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="GET_IMPORTED_FIELDS" SCONAME="RE_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Table with Fields" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZWIZ_T_FIELDS"/>
   <source>METHOD get_imported_fields.

  DATA:
    lr_item TYPE REF TO cl_salv_item,
    lr_data TYPE REF TO data.

  DATA:
    lt_tree TYPE salv_t_nodes,
    ls_leaf TYPE salv_s_nodes.

  FIELD-SYMBOLS:
    &lt;lf_table_field&gt;    TYPE zwiz_table_field,
    &lt;lf_imported_field&gt; TYPE zwiz_fields.

  TRY.
      lt_tree = nodes-&gt;get_all_nodes( ).

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

  LOOP AT lt_tree INTO ls_leaf.
    lr_data = ls_leaf-node-&gt;get_data_row( ).

    ASSIGN lr_data-&gt;* TO &lt;lf_table_field&gt;.

    READ TABLE imported_fields ASSIGNING &lt;lf_imported_field&gt;
      WITH KEY alias     = &lt;lf_table_field&gt;-alias
               fieldname = &lt;lf_table_field&gt;-fieldname.

    CHECK sy-subrc = 0.

    TRY.
        lr_item = ls_leaf-node-&gt;get_item( &apos;OUTPUT&apos; ).
        &lt;lf_imported_field&gt;-output = lr_item-&gt;is_checked( ).

      CATCH cx_salv_msg INTO salv_msg.
        MESSAGE salv_msg TYPE &apos;E&apos;.
    ENDTRY.
  ENDLOOP.

  re_fields[] = imported_fields[].

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="IS_PERSISTENT" VERSION="1" LANGU="E" DESCRIPT="Check at least one field have been checked" EXPOSURE="2" STATE="1" EDITORDER="11 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="IS_PERSISTENT" SCONAME="RE_PERSISTENT" VERSION="1" LANGU="E" DESCRIPT="Checkbox (yes or no)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="TDBOOL"/>
   <source>METHOD IS_PERSISTENT.

  DATA:
    lr_item TYPE REF TO cl_salv_item.

  DATA:
    lt_tree TYPE salv_t_nodes,
    ls_leaf TYPE salv_s_nodes.

  TRY.
      lt_tree = nodes-&gt;get_all_nodes( ).

    CATCH cx_salv_msg INTO salv_msg.
      MESSAGE salv_msg TYPE &apos;E&apos;.
  ENDTRY.

  LOOP AT lt_tree INTO ls_leaf.
    TRY.
        lr_item = ls_leaf-node-&gt;get_item( &apos;OUTPUT&apos; ).
        IF lr_item-&gt;is_checked( ) = abap_true.
          re_persistent = abap_true. RETURN.
        ENDIF.

      CATCH cx_salv_msg INTO salv_msg.
        MESSAGE salv_msg TYPE &apos;E&apos;.
    ENDTRY.
  ENDLOOP.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="PROCESS_COLUMNS" VERSION="1" LANGU="E" DESCRIPT="Process Treeview Columns" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDBY="RICH" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS_COLUMNS.

  DATA lr_column TYPE REF TO cl_salv_column.

  columns = tree-&gt;get_columns( ).

*  columns-&gt;set_optimize( abap_true ).

  TRY.
      lr_column = columns-&gt;get_column( &apos;FIELDNAME&apos; ).
      lr_column-&gt;set_output_length( 12 ).

      lr_column = columns-&gt;get_column( &apos;OUTPUT&apos; ).
      lr_column-&gt;set_output_length( 10 ).

      lr_column = columns-&gt;get_column( &apos;ALIAS&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;TABNAME&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;TABLE_TEXT&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;FIELD_TEXT&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;KEYFLAG&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;SELECTION&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;PARAMETER&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;OBLIGATORY&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

      lr_column = columns-&gt;get_column( &apos;NO_SELECTION&apos; ).
      lr_column-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).

    CATCH cx_salv_not_found.
  ENDTRY.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="PROCESS_FUNCTIONS" VERSION="1" LANGU="E" DESCRIPT="Process Treeview Functions" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS_FUNCTIONS.

  functions = tree-&gt;get_functions( ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="PROCESS_NODES" VERSION="1" LANGU="E" DESCRIPT="Process Treeview Nodes" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS_NODES.

  DATA l_node_key TYPE salv_de_node_key.

  FIELD-SYMBOLS &lt;lf_table_field&gt; TYPE zwiz_table_field.

  nodes = tree-&gt;get_nodes( ).

  LOOP AT table_fields ASSIGNING &lt;lf_table_field&gt;.
    AT NEW tabname.
      l_node_key = add_table_node( &lt;lf_table_field&gt; ).
    ENDAT.

    add_field_node( EXPORTING im_data_row     = &lt;lf_table_field&gt;
                              im_related_node = l_node_key ).
  ENDLOOP.

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="PROCESS_TREE_SETTINGS" VERSION="1" LANGU="E" DESCRIPT="Process Treeview Settings" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS_TREE_SETTINGS.

  settings = tree-&gt;get_tree_settings( ).

  settings-&gt;set_hierarchy_header( text-h01 ).
  settings-&gt;set_hierarchy_tooltip( text-h01 ).
  settings-&gt;set_hierarchy_size( 45 ).
  settings-&gt;set_header( co_title ).

ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_OUTPUT_FIELDS" CMPNAME="REFORMAT_FIELDS" VERSION="1" LANGU="E" DESCRIPT="Reformat IT for Treeview Display" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070924" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD reformat_fields.

  DATA:
    ls_table_field TYPE zwiz_table_field,
    l_table_text   TYPE dd02t-ddtext.

  FIELD-SYMBOLS
    &lt;lf_imported_field&gt; TYPE zwiz_fields.

  LOOP AT imported_fields ASSIGNING &lt;lf_imported_field&gt;.
    AT NEW alias.
      SELECT SINGLE ddtext FROM dd02t INTO l_table_text
        WHERE tabname    = &lt;lf_imported_field&gt;-tabname
          AND ddlanguage = sy-langu.

      IF &lt;lf_imported_field&gt;-alias &lt;&gt; &lt;lf_imported_field&gt;-tabname.
        CONCATENATE &lt;lf_imported_field&gt;-alias l_table_text
          INTO l_table_text SEPARATED BY space.
      ENDIF.
    ENDAT.

    MOVE-CORRESPONDING &lt;lf_imported_field&gt; TO ls_table_field.

    ls_table_field-table_text = l_table_text.
    ls_table_field-field_text = &lt;lf_imported_field&gt;-scrtext_m.

    APPEND ls_table_field TO table_fields.

    AT END OF tabname.
      CLEAR l_table_text.
    ENDAT.
  ENDLOOP.

ENDMETHOD.</source>
  </method>
 </CLAS>
