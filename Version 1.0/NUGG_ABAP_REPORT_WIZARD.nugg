  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_SELECT" VERSION="1" LANGU="E" DESCRIPT="Get code for Select Statement" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_SELECT" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code_select.

*SAP Query (S): T Type DBAN Table of Aliases
*SAP Query (S): T Type DBJT Table Join (Tables)
*SAP Query (S): T Type DBJC Table Join (Conditions)
*SAP Query (S): T Type DBSG Structure of Log Database
*Table of used Tables
*SAP Query (S): T Type SGTEXT Textpool for Functional Areas

  data: wa_line                   type string,
        wa_line2                  type string,
        wa_main_table             type aqdbjt,
        wa_left_table             type aqdbjt,
        wa_dbjc                   type aqdbjc,
        wa_next_dbjc              type aqdbjc,
        wa_dban                   type aqdban,
        wa_alias                  type aqdban,
        wa_field                  type zwiz_fields,
        l_alias                   type aqs_tname,
        l_length_total            type i,
        l_joins                   type i,
        l_ind_def                 type i,
        l_conditions              type i,
        l_ind_sel                 type i,
        l_end                     type char3,
        l_max_length              type i,
        l_eq                      type char2,
        l_not_first               type cmpflag,
        l_field                   type fieldname,
        l_counter                 type i,
        l_ind_into                type i,
        l_ind_from                type i,
        l_index                   type sytabix,
        l_ind_where               type i,
        l_cond_count              type i,
        l_offset                  type i,
        l_tabname                 type tabname,
        l_more_conditions_exist   type char1.

  constants: c_open               type char1 value &apos;(&apos;,
             c_close              type char1 value &apos;)&apos;,
             c_and                type char3 value &apos;AND&apos;,
             c_select             type char6 value &apos;SELECT&apos;,
             c_from               type char4 value &apos;FROM&apos;,
             c_inner              type char10 value &apos;INNER JOIN&apos;,
             c_outer              type char15 value &apos;LEFT OUTER JOIN&apos;.

  field-symbols: &lt;field&gt;      type zwiz_fields.

  check join_attributes-dbjt is not initial.

  l_ind_sel   = l_ind_def.
  l_ind_into  = l_ind_from = l_ind_sel + 2.
  l_ind_where = l_ind_sel + 1.

  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  wa_line = &apos;*       D A T A   S E L E C T I O N           *&apos;.   append wa_line to code.
  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  wa_line = &apos;START-OF-SELECTION.&apos;.   append wa_line to code.
  clear wa_line. append wa_line to code.

* Select
  wa_line = c_select.
  loop at fields assigning &lt;field&gt; where output = abap_true.
    read table join_attributes-join_alias into wa_dban with key table = &lt;field&gt;-alias.
    if sy-subrc = 0.
      concatenate wa_dban-alias &lt;field&gt;-fieldname into wa_line2 separated by &apos;~&apos;.
    endif.
    concatenate wa_line wa_line2 into wa_line separated by space.
    if strlen( wa_line ) &gt; 70.
      if wa_line(6) ne c_select.
        shift wa_line right by 6 places.
      endif.
      append wa_line to code.
      clear wa_line.
    endif.
  endloop.

  if wa_line is not initial.
    append wa_line to code.
    clear wa_line.
  endif.

* From
  read table join_attributes-dbjt into wa_main_table index 1.
  describe table join_attributes-dbjt lines l_joins.
  l_joins = l_joins - 1. &quot; Remove base table from the calculation

  wa_line = c_from.
  do l_joins times.
    concatenate wa_line c_open into wa_line separated by space.
  enddo.

  shift wa_line right by 2 places.

  read table join_attributes-join_alias into wa_dban with key table = wa_main_table-table.
  concatenate wa_line wa_main_table-table &apos;AS&apos; wa_dban-alias into wa_line separated by space.
  append wa_line to code.

* Process all possible joins
  loop at join_attributes-dbjt into wa_left_table.
    if wa_left_table-outerflag = &apos;I&apos;.
      wa_line = c_inner.
    else.
      wa_line = c_outer.
    endif.
    shift wa_line right by 11 places.

    loop at join_attributes-dbjc_work into wa_dbjc where ltable = wa_left_table-table and jind &gt; 0.
      l_index = sy-tabix + 1.
      l_cond_count = l_cond_count + 1.
      if l_cond_count = 1.
        read table join_attributes-join_alias into wa_alias with key table = wa_dbjc-rtable.
        read table join_attributes-dban       into wa_dban  with key alias = wa_dbjc-rtable.
        if sy-subrc = 0.
          l_tabname = wa_dban-table.
        else.
          l_tabname = wa_dbjc-rtable.
        endif.
        concatenate wa_line l_tabname &apos;AS&apos; wa_alias-alias &apos;ON&apos; wa_dbjc-lname &apos;=&apos; wa_dbjc-rname into wa_line separated by space.
        find wa_dbjc-lname in wa_line match offset l_offset.
      else.
        concatenate wa_dbjc-lname &apos;=&apos; wa_dbjc-rname into wa_line separated by space.
        shift wa_line right by l_offset places.
      endif.
      clear l_more_conditions_exist.
      loop at join_attributes-dbjc_work into wa_next_dbjc from l_index.
        if wa_next_dbjc-ltable = wa_left_table-table and wa_dbjc-jind &gt; 0.
          l_more_conditions_exist = abap_true.
          exit.
        else.
          exit.
        endif.
      endloop.
      if l_more_conditions_exist eq abap_false.
        concatenate wa_line c_close into wa_line separated by space.
      else.
        concatenate wa_line c_and into wa_line separated by space.
      endif.
      append wa_line to code.
      clear wa_line.
      clear l_more_conditions_exist.
    endloop.
    clear l_cond_count.
  endloop.

* Into
  loop at fields into wa_field where selection = abap_true.
    l_conditions = l_conditions + 1.
    read table join_attributes-join_alias into wa_dban with key table = wa_field-tabname.
    if sy-subrc = 0.
      l_length_total = ( strlen( wa_dban-alias ) + strlen( wa_field-tabname ) ).
      if l_max_length &lt; l_length_total.
        l_max_length = l_length_total.
      endif.
    endif.
  endloop.
  l_max_length = l_max_length + 2.

  if l_conditions = 0.
    wa_line = &apos;INTO TABLE IT_DATA.   &quot; &lt;--  Attention ! Field sequence !&apos;.
  else.
    wa_line = &apos;INTO TABLE IT_DATA   &quot; &lt;--  Attention ! Field sequence !&apos;.
  endif.
  shift wa_line by l_ind_into places right in character mode.
  append wa_line to code.

* Where
  loop at fields into wa_field where selection = abap_true.
    l_counter = l_counter + 1.

    read table join_attributes-join_alias into wa_dban with key table = wa_field-tabname.
    if sy-subrc = 0.
      l_alias = wa_dban-alias.
    else.
      l_alias = wa_field-tabname.
    endif.
    if l_counter = l_conditions.
      l_end = &apos;.&apos;.
    else.
      l_end = &apos;AND&apos;.
    endif.

    if wa_field-parameter = abap_true.
      l_eq = &apos;EQ&apos;.
      l_field = wa_field-p_name.
    else.
      l_eq = &apos;IN&apos;.
      l_field = wa_field-so_name.
    endif.
    concatenate l_alias &apos;~&apos; wa_field-fieldname into wa_line.
    concatenate l_eq l_field l_end into wa_line2 separated by space.
    l_offset = l_max_length - strlen( wa_line2 ).
    shift wa_line2 by l_offset places right in character mode.
    if l_not_first = abap_false.
      concatenate &apos;WHERE&apos; wa_line wa_line2 into wa_line separated by space.
      shift wa_line by 1 places right in character mode.
      l_not_first = abap_true.
    else.
      concatenate wa_line wa_line2 into wa_line separated by space.
      shift wa_line by 7 places right in character mode.
    endif.
    shift wa_line by l_ind_where places right in character mode.
    append wa_line to code.
  endloop.

  clear wa_line. append wa_line to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_SELECTION_SCREEN" VERSION="1" LANGU="E" DESCRIPT="Get code for Selection screen" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_SELECTION_SCREEN" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code_selection_screen.

  data: fields_subset             type zwiz_t_fields,
        wa_line                   type string,
        l_string1                 type string,
        l_string2                 type string,
        l_block                   type string,
        l_length                  type i,
        l_length_max              type i,
        l_offset                  type i,
        l_counter                 type i,
        l_counter_txt             type char2,
        wa_field                  type zwiz_fields.

  constants: c_dash               type char1 value &apos;-&apos;.

  fields_subset = fields.
  delete fields_subset where selection = abap_false.

  if fields_subset is initial.
    return.
  endif.

  sort fields_subset by alias tabname fieldname.

  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  wa_line = &apos;*      S E L E C T I O N   S C R E E N        *&apos;.   append wa_line to code.
  wa_line = &apos;*---------------------------------------------*&apos;.   append wa_line to code.
  clear wa_line. append wa_line to code.

* Get max size of field
  loop at fields_subset into wa_field.
    if wa_field-parameter eq abap_false.
      l_length = strlen( wa_field-so_name ) + 16.
    else.
      l_length = strlen( wa_field-p_name ) + 16.
    endif.
    if l_length &gt; l_length_max.
      l_length_max = l_length.
    endif.
  endloop.
  l_length_max = l_length_max + 2.

  clear l_counter.

  loop at fields_subset into wa_field.
    at new alias.
      l_counter = l_counter + 1.
      l_counter_txt = l_counter.
      unpack l_counter_txt to l_counter_txt.
      concatenate &apos;B&apos; l_counter_txt      into l_block.
      concatenate &apos;TITLE&apos; l_counter_txt  into l_string2.
      concatenate &apos;SELECTION-SCREEN: Begin of block&apos; l_block &apos;WITH FRAME TITLE&apos; l_string2 into wa_line separated by space.
      concatenate wa_line &apos;.&apos; into wa_line.
      append wa_line to code.

      call method me-&gt;populate_initialization
        exporting
          alias = wa_field-alias
          title = l_string2.
    endat.
    if wa_field-parameter eq abap_false.
      concatenate &apos;SELECT-OPTIONS:&apos; wa_field-so_name into l_string1 separated by space.
      l_offset = l_length_max - strlen( wa_field-so_name ) + 12.
      concatenate wa_field-tabname wa_field-fieldname into l_string2 separated by c_dash.
      concatenate &apos;FOR&apos; l_string2 into l_string2 separated by space.
    else.
      l_string1 = wa_field-p_name.
      shift l_string1 by 4 places right in character mode.
      concatenate &apos;PARAMETERS:&apos; l_string1 into l_string1 separated by space.
      l_offset = l_length_max - strlen( wa_field-p_name ) + 12.
      l_string2 = wa_field-rollname.
      concatenate &apos;TYPE&apos; l_string2 into l_string2 separated by space.
    endif.

    if wa_field-obligatory eq abap_true.
      concatenate l_string2 &apos;OBLIGATORY&apos; into l_string2 separated by space.
    endif.

    concatenate l_string2 &apos;.&apos; into l_string2.
    shift l_string2 by l_offset places right in character mode.
    concatenate l_string1 l_string2 into wa_line.
*    shift wa_line by 6 places right in character mode.
    append wa_line to code.
    at end of alias.
      concatenate &apos;SELECTION-SCREEN: end of block&apos; l_block into wa_line separated by space.
      concatenate wa_line &apos;.&apos; into wa_line.
      append wa_line to code.
    endat.
  endloop.

  clear wa_line. append wa_line to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_TABLES" VERSION="1" LANGU="E" DESCRIPT="Get code for Tables statement" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_CODE_TABLES" SCONAME="CODE" VERSION="1" LANGU="E" DESCRIPT="ABAP Source" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="RSWSOURCET"/>
   <source>method get_code_tables.

  data: wa_line                   type string,
        wa_table                  type aqdbjt,
        wa_field                  type zwiz_fields,
        it_fields_subset          type zwiz_t_fields,
        l_tables                  type i,
        l_end                     type char1.

  read table fields with key output = abap_true transporting no fields.
  if sy-subrc ne 0.
    return.
  endif.

  wa_line = &apos;* Tables ( To support selection screen )&apos;.
  append wa_line to code.
  wa_line = &apos;TABLES:&apos;.
  append wa_line to code.

  it_fields_subset = fields.
  sort it_fields_subset by tabname.
  delete adjacent duplicates from it_fields_subset comparing tabname.

  describe table it_fields_subset lines l_tables.
  loop at it_fields_subset into wa_field.
    if sy-tabix = l_tables.
      l_end = &apos;.&apos;.
    else.
      l_end = &apos;,&apos;.
    endif.
    concatenate wa_field-tabname l_end into wa_line.
    shift wa_line by 7 places right in character mode.
    append wa_line to code.
  endloop.
  clear wa_line. append wa_line to code.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="GET_FIELDS_INVOLVED" VERSION="1" LANGU="E" DESCRIPT="Get table with all fields involved" EXPOSURE="2" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070904" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
   <source>method get_fields_involved.

  data: wa_dbjt            type aqdbjt,
        wa_dbjt_right      type aqdbjt,
        wa_dbjc            type aqdbjc,
        wa_dban            type aqdban,
        l_count            type i,
        l_count_txt        type char2,
        l_char8            type char8,
        l_dup_flag         type char1,
        l_offset           type i,
        o_struct           type ref to cl_abap_structdescr,
        it_fields_def      type table of dfies,
        wa_fields_def      type dfies,
        it_fields_tmp      type zwiz_t_fields,
        wa_fields_tmp      type zwiz_fields.

  field-symbols: &lt;field&gt;   type zwiz_fields,
                 &lt;field2&gt;  type zwiz_fields.

  clear fields.

  loop at join_attributes-dbjt into wa_dbjt.
    clear it_fields_tmp.
    read table join_attributes-dban into wa_dban with key alias = wa_dbjt-table.
    if sy-subrc = 0.
      o_struct ?= cl_abap_tabledescr=&gt;describe_by_name( wa_dban-table ).
    else.
      o_struct ?= cl_abap_tabledescr=&gt;describe_by_name( wa_dbjt-table ).
    endif.
    it_fields_def = o_struct-&gt;get_ddic_field_list( p_langu = sy-langu ).
    loop at it_fields_def into wa_fields_def.
      move-corresponding wa_fields_def to wa_fields_tmp.
      wa_fields_tmp-alias = wa_dbjt-table.
      append wa_fields_tmp to it_fields_tmp.
    endloop.
* Pass flag for non selection possible for right tables of a Left Outer join
    read table join_attributes-dbjc into wa_dbjc with key jind = &apos;&apos;
                                                          rtable = wa_dbjt-table.
    if sy-subrc = 0.
      read table join_attributes-dbjt into wa_dbjt_right with key table = wa_dbjc-ltable.
      if sy-subrc = 0 and wa_dbjt_right-outerflag = &apos;L&apos;.
        loop at it_fields_tmp assigning &lt;field&gt;.
          &lt;field&gt;-no_selection = abap_true.
        endloop.
      endif.
    endif.
    append lines of it_fields_tmp to fields.
  endloop.

* Client specification is not yet supported. With time we have to implement the
* generation of CLIENT SPECIFIED selections
  delete fields where fieldname = &apos;MANDT&apos;.

* Rename all target fields if field appears more than once.
  loop at fields assigning &lt;field&gt;.
    &lt;field&gt;-target_fieldname = &lt;field&gt;-fieldname.
  endloop.
  loop at fields assigning &lt;field&gt;.
    clear l_dup_flag.
    loop at fields assigning &lt;field2&gt; where target_fieldname = &lt;field&gt;-target_fieldname
                                        and not ( tabname   eq &lt;field&gt;-tabname and
                                                  alias     eq &lt;field&gt;-alias ).
      l_dup_flag = abap_true.
      concatenate &lt;field2&gt;-alias
                  &lt;field2&gt;-fieldname
                  into &lt;field2&gt;-target_fieldname separated by &apos;_&apos;.

    endloop.
    if l_dup_flag = abap_true.
      concatenate &lt;field&gt;-alias
                  &lt;field&gt;-fieldname
                  into &lt;field&gt;-target_fieldname separated by &apos;_&apos;.
    endif.
  endloop.

* Name Parameters
  loop at fields assigning &lt;field&gt;.
    l_count = 1.
    concatenate &apos;P_&apos; &lt;field&gt;-fieldname into l_char8.
    if sy-subrc = 4.
      do.
        l_count_txt  = l_count.
        l_offset     = strlen( l_char8 ).
        if l_offset &lt; 7.
          concatenate l_char8 l_count_txt into l_char8.
        else.
          if strlen( l_count_txt ) &gt; 1.
            l_char8+6(2) = l_count_txt.
          else.
            l_char8+7(1) = l_count_txt.
          endif.
        endif.
* Check if new field doesn&apos;t already exist
        read table fields with key p_name = l_char8 transporting no fields.
        if sy-subrc ne 0.
          &lt;field&gt;-p_name = l_char8.
          exit.
        else.
          l_count = l_count + 1.
        endif.
      enddo.
    else.
      do.
* Check if new field doesn&apos;t already exist
        read table fields with key p_name = l_char8 transporting no fields.
        if sy-subrc ne 0.
          &lt;field&gt;-p_name = l_char8.
          exit.
        else.
          l_count = l_count + 1.
          l_count_txt = l_count.
          if strlen( l_count_txt ) &gt; 1.
            l_char8+6(2) = l_count_txt.
          else.
            l_char8+7(1) = l_count_txt.
          endif.
        endif.
      enddo.
    endif.
  endloop.

* Name Select_options
  loop at fields assigning &lt;field&gt;.
    l_count = 1.
    concatenate &apos;SO_&apos; &lt;field&gt;-fieldname into l_char8.
    if sy-subrc = 4.
      do.
        l_count_txt  = l_count.
        l_offset     = strlen( l_char8 ).
        if l_offset &lt; 7.
          concatenate l_char8 l_count_txt into l_char8.
        else.
          if strlen( l_count_txt ) &gt; 1.
            l_char8+6(2) = l_count_txt.
          else.
            l_char8+7(1) = l_count_txt.
          endif.
        endif.
* Check if new field doesn&apos;t already exist
        read table fields with key so_name = l_char8 transporting no fields.
        if sy-subrc ne 0.
          &lt;field&gt;-so_name = l_char8.
          exit.
        else.
          l_count = l_count + 1.
        endif.
      enddo.
    else.
      do.
* Check if new field doesn&apos;t already exist
        read table fields with key so_name = l_char8 transporting no fields.
        if sy-subrc ne 0.
          &lt;field&gt;-so_name = l_char8.
          exit.
        else.
          l_count = l_count + 1.
          l_count_txt = l_count.
          if strlen( l_count_txt ) &gt; 1.
            l_char8+6(2) = l_count_txt.
          else.
            l_char8+7(1) = l_count_txt.
          endif.
        endif.
      enddo.
    endif.
  endloop.

endmethod.</source>
  </method>
  <method CLSNAME="ZCL_BT_ABAP_CODE" CMPNAME="INITIALIZE_ALL" VERSION="1" LANGU="E" DESCRIPT="Clear all variables (not title)" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="LEONARDO" CREATEDON="20070925" CHANGEDBY="LEONARDO" CHANGEDON="20070926" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="700" BCMTDCAT="00" BCMTDSYN="0">
